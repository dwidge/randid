{
  "name": "@dwidge/randid",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwidge/randid.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
