    "test": "node --test"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
